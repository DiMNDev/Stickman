function dbl_jump_SCR() 
{
while image_index <= 15 {

	flick = 0;
	dbl_jump = false;
	if face_left = 1 sprite_index = Stickman_jumpL;
	if face_right = 1 sprite_index = Stickman_jumpR;

}
}